{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "padrao": {
      "format": "%(asctime)s | %(name)s | %(levelname)s | %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "padrao",
      "stream": "ext://sys.stdout"
    },
    "arquivo": {
      "class": "logging.FileHandler",
      "level": "INFO",
      "formatter": "padrao",
      "filename": "./logging/logs/json_config.log",
      "encoding": "utf8"
    }
  },
  "loggers": {
    "app": {
      "level": "DEBUG",
      "handlers": ["console", "arquivo"],
      "propagate": false
    },
    "modulo.db": {
      "level": "WARNING",
      "handlers": ["arquivo"],
      "propagate": false
    }
  },
  "root": {
    "level": "WARNING",
    "handlers": ["console"]
  }
}
